template(name="orgaTransactions")
  #main.ui.feed
    each transactions
      .event
        .label
          a(href="/people/#{sender._id}")
            img(src='#{sender.profile.picture}')
        .content
          .date #{createdAt}
          .summary
            a(href="/people/#{sender._id}") #{sender.username}
            | has given #{mangos} Mangos
          .exta.text #{message}
